                width: 100%;
                height: auto;
            }

        .carousel-wrapper {
            margin: 20px auto;
            max-width: 1200px;
            padding: 0 20px;
        }

        .carousel-item img {
            width: 100%;
            border-radius: 10px;
        }

        .carousel-item {
            text-align: center;
            padding: 10px;
        }

        .owl-carousel .item {
            padding: 10px;
            box-sizing: border-box;
        }

        .owl-carousel .owl-item img {
            width: 100%;
            height: auto;
        }

        .owl-stage-outer {
            text-align: center;
        }

        .introbox1 {
            display: flex;
            justify-content: center;
            align-items: flex-start;
            margin: 30px 0;
        }

        .introbox1l {
            max-width: 50%;
            width: 100%;
            padding: 20px;
        }

        .introbox1__tit h3,
        .introbox1__tit h2 {
            text-align: center;
            margin: 10px 0;
            color: #f48120;
        }

        .introbox1__content {
            text-align: justify;
            line-height: 1.6;
            font-size: 16px;
        }

        .image_box {
            display: flex;
            justify-content: center;
            align-items: center;
            max-width: 50%;
        }

        .owl-carousel {
            display: block;
        }

        .owl-item {
            display: flex;
            justify-content: center;
        }

            .owl-item img {
                max-width: 100%;
                height: auto;
            }

        .owl-nav {
            display: flex;
            justify-content: space-between;
            width: 100%;
            position: absolute;
            top: 50%;
        }

        .introbox5 {
            display: flex;
            justify-content: center;
            align-items: flex-start;
            padding: 20px;
        }

        .introbox1__tit h3,
        .introbox1__tit h2 {
            color: #f48120;
        }

        .container {
            width: 80%;
            margin: 0 auto;
        }

        .section-title {
            text-align: center;
            margin-bottom: 30px;
        }

            .section-title h2 {
                font-size: 36px;
                color: #f26c4f;
                font-weight: bold;
            }

            .section-title h3 {
                font-size: 24px;
                color: #333;
            }

        .card-container {
            display: flex;
            justify-content: space-between;
            gap: 20px;
        }

        .card {
            background-color: #333;
            color: #f5c56a;
            width: 18%;
            border-radius: 10px;
            box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.1);
            overflow: hidden;
            transition: transform 0.3s ease-in-out;
        }

            .card:hover {
                transform: scale(1.05);
            }

        .card-header {
            background-color: #f5c56a;
            color: #333;
            padding: 10px;
            font-size: 24px;
            font-weight: bold;
            text-align: center;
        }

        .card-body {
            padding: 15px;
            text-align: center;
        }

            .card-body h4 {
                font-size: 20px;
                margin-bottom: 10px;
            }

            .card-body p {
                font-size: 16px;
            }

    </style>
</head>

